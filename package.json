{
  "name": "naotimes-panel",
  "version": "0.2.0",
  "copyright": {
    "name": "naoTimesDev",
    "since": 2019
  },
  "description": "A web panel for naoTimes Showtimes Module",
  "main": "dist/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp",
    "build:dev": "cross-env NODE_ENV=development gulp",
    "watch": "cross-env TAILWIND_MODE=build NODE_ENV=development gulp -f gulp-watch.js",
    "watch:dev": "npm-run-all build:dev watch",
    "prod": "npm-run-all build prod:start",
    "prod:start": "cross-env NODE_ENV=production node .",
    "dev": "npm-run-all build:dev dev:start",
    "dev:start": "nodemon ./src/index.ts",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint --ext ts --ext tsx --ext js src/ lib/",
    "lintfix": "eslint --ext ts --ext tsx --ext js src/ lib/ --fix",
    "testall": "npm-run-all lint type-check",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naoTimesdev/webpanel.git"
  },
  "keywords": [
    "nextjs"
  ],
  "author": "N4O",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naoTimesdev/webpanel/issues"
  },
  "homepage": "https://github.com/naoTimesdev/webpanel#readme",
